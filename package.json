{
  "name": "@emilecalixte/ffa-categories",
  "version": "0.2.2",
  "description": "Package utilitaire permettant de manipuler les catégories de la Fédération Française d'Athlétisme",
  "author": "Emile Calixte",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmileCalixte/ffa-categories.git"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier \"./**/*.{js,ts,json}\" --write",
    "lint-path": "eslint --max-warnings=0 --no-warn-ignored",
    "lint": "pnpm lint-path .",
    "lint-fix": "pnpm lint --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "pnpm format",
      "pnpm lint-fix"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ffa",
    "fédération française d'athlétisme",
    "france",
    "utilities"
  ],
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631",
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3",
    "eslint": "^9.16.0",
    "eslint-config-love": "^119.0.0",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.0.2"
  }
}
